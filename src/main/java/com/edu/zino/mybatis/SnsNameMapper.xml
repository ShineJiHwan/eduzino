<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="SnsName">

	<!--전체 가져오기 -->
	<select id="selectAll" resultType="SnsName">
		select * from sns_name order by sns_name_idx asc
	</select>

	<!--멤버idx로 가져오기 -->	
	<select id="selectByMember" parameterType="int" resultType="SnsName">
		select * from sns_name 
		where member_idx=#{member_idx}
	</select>

	<!--전체 가져오기 -->	
	<select id="selectByType" parameterType="int" resultType="SnsName">
	 	select * from sns_name where sns_type=#{sns_type}	
	</select>

</mapper>