<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="SnsName">

	<!--멤버idx로 가져오기 -->	
	<select id="selectByMember" parameterType="int" resultType="SnsName">
		select * from snsName where member_idx=#{member_idx}
	</select>

	<!--전체 가져오기 -->	
	<select id="selectByType" parameterType="int" resultType="SnsName">
	 	select * from snsName where snsType=#{snsType}	
	</select>

<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
		
	
	<!--전체 가져오기 -->
	<select id="selectAll" resultType="SnsName">
		select * from snsName order by snsName_idx asc
	</select>
	
	
	<insert id="insert" parameterType="SnsName">
		insert into snsName(snsType) values(#{snsType})
	</insert>	

	
	<delete id="delete" parameterType="int">
		delete from category where snsName_idx=#{snsName_idx}
	</delete>	
	

</mapper>